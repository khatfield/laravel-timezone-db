{
  "name": "khatfield/laravel-timezone-db",
  "description": "Laravel Package for TimezoneDB API",
  "license": "MIT",
  "authors": [
    {
      "name": "Keith Hatfield",
      "email": "keith.hatfield@gmail.com"
    }
  ],
  "minimum-stability": "dev",
  "repositories": [{
    "type": "composer",
    "url": "https://repos.hatfield.me"
  }],
  "require": {
    "php": "^7.1.3",
    "ext-json": "*"
  },
  "autoload": {
    "psr-4": {
      "Khatfield\\LaravelTimezoneDb\\": "src/"
    }
  },
  "extra": {
    "laravel": {
      "providers": [
        "Khatfield\\LaravelTimezoneDb\\Providers\\TimezoneDbServiceProvider"
      ],
      "aliases": {
        "TimezoneDb": "Khatfield\\LaravelTimezoneDb\\Facades\\TimezoneDb"
      }
    }
  }
}
